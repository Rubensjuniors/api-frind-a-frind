    "@vitest/ui": "2.1.6",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.1",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "npm-run-all": "4.1.5",
    "prisma": "6.0.0",
    "supertest": "7.0.0",
    "typescript": "^5.7.2",
